<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="web.Terminal">
        <div class="o-web-terminal">
               <div class="o-web-terminal-title">
                   <h6>Odoo</h6>
                   <span class="fa fa-close close" />
               </div>
               <div id="terminal" />
        </div>

    </t>
    <t t-name="web.TerminalMenu">
        <li role="menuitem" class="o-web-shell">
            <a href="javascript:void(0)" role="button">
                <span class="fa fa-terminal" />
            </a>
        </li>
    </t>
    <t t-name="web.TerminalEntries">
        <div class="o-web-terminal-entries">
            <ul>
                <t t-foreach="entries" t-as="entry">
                    <li class="o-web-terminal-entry">
                       <span><t t-esc="entry" /></span>
                    </li>
                </t>
            </ul>
        </div>
    </t>
</templates>
